{
  "compilerOptions": {
    "target": "es2018",
    "module": "commonjs",
    "outDir": "app",
    "moduleResolution": "node",
    "declaration": true,
    "sourceMap": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "forceConsistentCasingInFileNames": true,
    "strictNullChecks": false,
    "skipLibCheck": true
  }
}
